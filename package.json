{
  "name": "vuejs-smart-table",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/vuejs-smart-table.umd.js",
  "module": "./dist/vuejs-smart-table.es.js",
  "types": "./dist/vuejs-smart-table.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vuejs-smart-table.es.js",
      "require": "./dist/vuejs-smart-table.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && yarn run tsDeclaration && yarn run vueDeclaration",
    "vueDeclaration": "vue-dts-gen --outDir dist/types \"src/*.vue\"",
    "tsDeclaration": "tsc -d --outDir dist/types --emitDeclarationOnly src/vuejs-smart-table.ts",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue": "^3.0.5",
    "vue-dts-gen": "^0.2.3",
    "vue-tsc": "^0.0.19"
  }
}
